{
    "extends": ["tslint:latest"],
    "rules": {
        "forin": false,
        "max-line-length": false,
        "member-ordering": false,
        "no-console": false,
        "no-string-literal": false,
        "no-unnecessary-initializer": false,
        "object-literal-key-quotes": false,
        "object-literal-sort-keys": false
    }
}